<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="NoteDisplayBackgroundPane" maxHeight="600.0" minHeight="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="building.notes_keeper.NoteDisplayController">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Rectangle fx:id="Background" arcHeight="25.0" arcWidth="25.0" fill="#202124" height="202.0" stroke="WHITE" strokeLineCap="BUTT" strokeLineJoin="ROUND" strokeType="OUTSIDE" strokeWidth="3.0" width="409.0" />
            <VBox>
               <children>
                  <Label fx:id="HeadingText" text="Label" textAlignment="CENTER" textFill="WHITE" wrapText="true" VBox.vgrow="SOMETIMES">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Label>
                  <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" VBox.vgrow="ALWAYS" />
                  <Label fx:id="NoteText" text="Label" textFill="WHITE" wrapText="true">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" VBox.vgrow="NEVER" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <AnchorPane>
               <children>
                  <HBox prefHeight="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <padding>
                        <Insets bottom="20.0" left="10.0" right="10.0" />
                     </padding>
                     <children>
                        <Hyperlink fx:id="pin" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Hyperlink fx:id="paint" />
                        <Pane prefHeight="200.0" HBox.hgrow="ALWAYS" />
                        <Hyperlink fx:id="copy" />
                        <Pane prefHeight="200.0" HBox.hgrow="ALWAYS" />
                        <Hyperlink fx:id="delete" />
                        <Pane prefHeight="200.0" HBox.hgrow="ALWAYS" />
                        <Hyperlink fx:id="more" />
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
